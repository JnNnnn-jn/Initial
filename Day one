int Max(int a, int b, int c)
{
	if (a > b)
	{
		if (a > c)
			return a;
		else if (c > a)
			return c;
	}
	else if (b > a)
	{
		if (b > c)
			return b;
		else if (c > b)
			return c;
	}
}
int main()
{
	int a, b, c,max;
	printf("请输入三个数：");
	scanf("%d %d %d", &a, &b, &c);
	max=Max(a, b, c);
	printf("最大的数是：%d\n", max);
	return 0;
}


#include<math.h>
int main()
{
	int a, b;
	printf("请输入一个小于1000的数：");
	scanf("%d", &a);
	while (a > 1000)
	{
		printf("输入的数据不符合要求，请重新输入一个小于1000的数：");
		scanf("%d", &a);
		
	}
	b = sqrt(a);
	printf("%d的平方根是%d\n", a, b);

	return 0;
}


