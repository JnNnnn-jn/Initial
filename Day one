#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>
#include<string.h>

//运用到了指针
void swap(int* pa, int* pb)
{
	int z = 0;
	z = *pa;
	*pa = *pb;
	*pb = z;
}
int main()
{
	int a = 10;
	int b = 20;
	printf("交换前：a=%d b=%d", a, b);
	swap(&a,&b);
	printf("交换后：a=%d b=%d", a, b);
	return 0;
}

